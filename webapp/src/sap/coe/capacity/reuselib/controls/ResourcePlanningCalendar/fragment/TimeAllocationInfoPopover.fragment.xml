                            <Text text="{i18n>TA_INFO_TABLE_ROW_EFFECT_ON_UTILIZATION_REMOTE_ONLY}" />
                        </cells>
                    </ColumnListItem>
                    <ColumnListItem>
                        <cells>
                            <Text text="{i18n>TA_INFO_TABLE_ROW_TIME_ALLOCATION_OFFICE_BLOCKED}" />
                            <Text text="{i18n>TA_INFO_TABLE_ROW_HOW_TO_USE_OFFICE_BLOCKED}" />
                            <Text text="{i18n>TA_INFO_TABLE_ROW_EFFECT_ON_CAPACITY_OFFICE_BLOCKED}" />
