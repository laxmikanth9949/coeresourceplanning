                            <Text text="{i18n>TA_INFO_TABLE_ROW_HOW_TO_USE_TENTATIVE_SERVICE}" />
                            <Text text="{i18n>TA_INFO_TABLE_ROW_EFFECT_ON_CAPACITY_TENTATIVE_SERVICE}" />
                            <Text text="{i18n>TA_INFO_TABLE_ROW_EFFECT_ON_UTILIZATION_TENTATIVE_SERVICE}" />
                        </cells>
                    </ColumnListItem>
                    <ColumnListItem>
                        <cells>
                            <Text text="{i18n>TA_INFO_TABLE_ROW_TIME_ALLOCATION_TRAINEE}" />
                            <Text text="{i18n>TA_INFO_TABLE_ROW_HOW_TO_USE_TRAINEE}" />
                            <Text text="{i18n>TA_INFO_TABLE_ROW_EFFECT_ON_CAPACITY_TRAINEE}" />
                            <Text text="{i18n>TA_INFO_TABLE_ROW_EFFECT_ON_UTILIZATION_TRAINEE}" />
                        </cells>
                    </ColumnListItem>
                    <ColumnListItem>
                        <cells>
                            <Text text="{i18n>TA_INFO_TABLE_ROW_TIME_ALLOCATION_TRAINER}" />
                            <Text text="{i18n>TA_INFO_TABLE_ROW_HOW_TO_USE_TRAINER}" />
                            <Text text="{i18n>TA_INFO_TABLE_ROW_EFFECT_ON_CAPACITY_TRAINER}" />
                            <Text text="{i18n>TA_INFO_TABLE_ROW_EFFECT_ON_UTILIZATION_TRAINER}" />
                        </cells>
                    </ColumnListItem>
            </Table>
            <Link emphasized="true" text="{i18n>TEXT_FOR_TA_JAM_PAGE_LINK}" press="onTimeAllocationJamLinkPress" id="taJamLink" />
