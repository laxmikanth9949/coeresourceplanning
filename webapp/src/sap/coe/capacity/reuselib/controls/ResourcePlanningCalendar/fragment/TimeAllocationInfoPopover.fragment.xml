                            <Text text="{i18n>TA_INFO_TABLE_ROW_HOW_TO_USE_RT_EXPERTISE}" />
                            <Text text="{i18n>TA_INFO_TABLE_ROW_EFFECT_ON_CAPACITY_RT_EXPERTISE}" />
