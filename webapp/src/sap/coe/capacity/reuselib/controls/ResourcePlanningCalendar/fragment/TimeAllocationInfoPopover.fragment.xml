<core:FragmentDefinition
    xmlns="sap.m" xmlns:f="sap.ui.layout.form" xmlns:l="sap.ui.layout"
    xmlns:core="sap.ui.core">
    <ResponsivePopover
        title="{i18n>TA_INFO_POPOVER_TITLE}"
        class="sapUiPopupWithPadding"
        placement="Auto" >
        <content>
            <Table selectionChange="onTableSelectionChange" fixedLayout="false" growing="true">
                <columns>
                    <Column id="scopeDate">
                        <Text text="{i18n>TA_INFO_TABLE_COLUMN_TIME_ALLOCATION}" />
                    </Column>
                    <Column id="cwCol">
                        <Text text="{i18n>TA_INFO_TABLE_COLUMN_HOW_TO_USE}" />
                    </Column>
                    <Column id="startDateCol" >
                        <Text text="{i18n>TA_INFO_TABLE_COLUMN_EFFECT_ON_CAPACITY}" />
                    </Column>
                    <Column id="startTimeCol" >
                        <Text text="{i18n>TA_INFO_TABLE_COLUMN_EFFECT_ON_UTILIZATION}" />
                    </Column>
                </columns>
                    <ColumnListItem>
                        <cells>
                            <Text text="{i18n>TA_INFO_TABLE_ROW_TIME_ALLOCATION_ABSENCE}" />
                            <Text text="{i18n>TA_INFO_TABLE_ROW_HOW_TO_USE_ABSENCE}" />
                            <Text text="{i18n>TA_INFO_TABLE_ROW_EFFECT_ON_CAPACITY_ABSENCE}" />
                            <Text text="{i18n>TA_INFO_TABLE_ROW_EFFECT_ON_UTILIZATION_ABSENCE}" />
                        </cells>
                    </ColumnListItem>
                    <ColumnListItem>
                        <cells>
                            <Text text="{i18n>TA_INFO_TABLE_ROW_TIME_ALLOCATION_BO_DUTY}" />
                            <Text text="{i18n>TA_INFO_TABLE_ROW_HOW_TO_USE_BO_DUTY}" />
                            <Text text="{i18n>TA_INFO_TABLE_ROW_EFFECT_ON_CAPACITY_BO_DUTY}" />
                            <Text text="{i18n>TA_INFO_TABLE_ROW_EFFECT_ON_UTILIZATION_BO_DUTY}" />
                        </cells>
                    </ColumnListItem>
                    <ColumnListItem>
                        <cells>
                            <Text text="{i18n>TA_INFO_TABLE_ROW_TIME_ALLOCATION_BILLABLE_PROJ_WORK}" />
                            <Text text="{i18n>TA_INFO_TABLE_ROW_HOW_TO_USE_BILLABLE_PROJ_WORK}" />
                            <Text text="{i18n>TA_INFO_TABLE_ROW_EFFECT_ON_CAPACITY_BILLABLE_PROJ_WORK}" />
                            <Text text="{i18n>TA_INFO_TABLE_ROW_EFFECT_ON_UTILIZATION_BILLABLE_PROJ_WORK}" />
                        </cells>
                    </ColumnListItem>
                    <ColumnListItem>
                        <cells>
                            <Text text="{i18n>TA_INFO_TABLE_ROW_TIME_ALLOCATION_ESCALATION}" />
                            <Text text="{i18n>TA_INFO_TABLE_ROW_HOW_TO_USE_ESCALATION}" />
                            <Text text="{i18n>TA_INFO_TABLE_ROW_EFFECT_ON_CAPACITY_ESCALATION}" />
                            <Text text="{i18n>TA_INFO_TABLE_ROW_EFFECT_ON_UTILIZATION_ESCALATION}" />
                        </cells>
                    </ColumnListItem>
                    <ColumnListItem>
                        <cells>
                            <Text text="{i18n>TA_INFO_TABLE_ROW_TIME_ALLOCATION_INTERNAL_PROJ_WORK}" />
                            <Text text="{i18n>TA_INFO_TABLE_ROW_HOW_TO_USE_INTERNAL_PROJ_WORK}" />
                            <Text text="{i18n>TA_INFO_TABLE_ROW_EFFECT_ON_CAPACITY_INTERNAL_PROJ_WORK}" />
                            <Text text="{i18n>TA_INFO_TABLE_ROW_EFFECT_ON_UTILIZATION_INTERNAL_PROJ_WORK}" />
                        </cells>
                    </ColumnListItem>
                    <ColumnListItem>
                        <cells>
                            <Text text="{i18n>TA_INFO_TABLE_ROW_TIME_ALLOCATION_REMOTE_ONLY}" />
                            <Text text="{i18n>TA_INFO_TABLE_ROW_HOW_TO_USE_REMOTE_ONLY}" />
